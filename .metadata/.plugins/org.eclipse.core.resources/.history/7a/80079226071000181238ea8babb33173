package com.eCommerce.Controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

@Controller
public class HomeController {
	
	@RequestMapping(value="/")
	public ModelAndView indexPage()
	{
		ModelAndView m1=new ModelAndView("index");
		return m1;
	}
	@RequestMapping(value="/login")
	public ModelAndView loginPage()
	{
		ModelAndView m2=new ModelAndView("login");
		return m2;
	}
	@RequestMapping(value="/signup")
	public ModelAndView signUpPage()
	{
		ModelAndView m3=new ModelAndView("signup");
		return m3;
	}
	@RequestMapping(value="/Exotic_Category")
	public ModelAndView exotic_CategoryPage()
	{
		ModelAndView m4=new ModelAndView("Exotic_Category");
		return m4;
	}@RequestMapping(value="/Cup_Category")
	public ModelAndView cup_CategoryPage()
	{
		ModelAndView m5=new ModelAndView("Cup_Category");
		return m5;
	}@RequestMapping(value="/Cone_Category")
	public ModelAndView cone_CategoryPage()
	{
		ModelAndView m6=new ModelAndView("Cone_Category");
		return m6;
	}@RequestMapping(value="/FamilyPack_Category")
	public ModelAndView familyPack_CategoryPage()
	{
		ModelAndView m7=new ModelAndView("FamilyPack_Category");
		return m7;
	}@RequestMapping(value="/Stick_Category")
	public ModelAndView stick_CategoryPage()
	{
		ModelAndView m8=new ModelAndView("Stick_Category");
		return m8;
	}
}
